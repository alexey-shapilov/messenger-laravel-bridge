{
    "name": "sa/messenger-laravel-bridge",
    "description": "",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.8 || ^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.5 || ^4.0",
        "phpunit/phpunit": " ^7.5 || ^8.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "SA\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\SA\\MessengerBridge\\Provider"
            ]
        }
    }
}
